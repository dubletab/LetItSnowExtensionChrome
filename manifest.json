{
	"manifest_version": 2,
	"name": "letItSnow",
	"version": "1.0",
	"browser_action": {
		"default_icon": "favicon.ico"
	},
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
				"https://github.com/*",
				"https://vk.com/*"
			],
			"js": [
				"PureSnow.js"
			]
		}
	]

}